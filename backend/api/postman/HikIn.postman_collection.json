			}
		]
	},
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"/**",
					" * SCRIPT DE AUTOMATIZACIÓN DE AUTENTICACIÓN BÁSICA EN POSTMAN",
					" * Va en la sección \"Pre-request script\" de la colección",
					" */",
					"",
					"// Importa la librería moment.js, integrada en Postman.",
					"const moment = require( 'moment' );",
					"",
					"// Establece los nombres de las variables.",
					"const testJWT            = 'testJWT';",
					"const testJWT_expiryDate = 'testJWT_expiryDate';",
					"",
					"// Decide si pedir un token en base a la información que ya tiene.",
					"let getToken;",
					"if( !pm.environment.has( testJWT ) || // No hay un token almacenado.",
					"    !pm.environment.has( testJWT_expiryDate ) || // No hay una fecha de caducidad para el token almacenado.",
					"    pm.environment.get( testJWT_expiryDate ) <= moment().valueOf() ) { // Se ha superado la fecha de caducidad.",
					"    getToken = true;",
					"}",
					"else {",
					"    getToken = false;",
					"}",
					"",
					"// Pide el token si así se ha decidido.",
					"if( getToken === true ){",
					"    console.log( 'Solicitando token nuevo...' );",
					"",
					"    // Crea el objeto de petición.",
					"    const JWTrequest = {",
					"        url: pm.environment.get( 'baseURL' ) + '/auth' ,",
					"        method: 'POST' ,",
					"        header: {",
					"            Accept: '*/*' ,",
					"            \"Content-Type\": 'application/json'",
					"        } ,",
					"        body: {",
					"            mode: 'raw',",
					"            raw: JSON.stringify( {",
					"                email: 'johndoe@mail.com' ,",
					"                contrasena: 'JohnDoe'",
					"            } )",
					"        }",
					"    };",
					"",
					"    // Manda la petición.",
					"    pm.sendRequest( JWTrequest , ( err , res ) => {",
					"        console.log( 'Respuesta recibida' );",
					"",
					"        if( ( err === null ) && ",
					"            ( res.code < 400 ) ){",
					"            try{",
					"                // Obtiene el cuerpo de la respuesta.",
					"                const responseToken = res.json().token;",
					"",
					"                // Establece como nuevo token el que ha recibido.",
					"                const jwt = responseToken.jwt;",
					"                pm.environment.set( testJWT , jwt );",
					"",
					"                console.log( `Token almacenado: ${jwt}` );",
					"    ",
					"                // Calcula la fecha de caducidad a partir del tiempo de vida recibido en milisegundos y la establece.",
					"                const expiryDate = moment().add( responseToken.expires , 'ms' ).valueOf();",
					"                pm.environment.set( testJWT_expiryDate , expiryDate );",
					"    ",
					"                console.log( `Fecha de caducidad almacenada (en ms): ${expiryDate}` );",
					"            } catch( errr ){",
					"                console.error( errr );",
					"                console.error( 'Ha habido un error al solicitar el token nuevo' );",
					"            }",
					"        }",
					"        else{",
					"            if( err ) console.error( err );",
					"            console.error( 'Ha habido un error al solicitar el token nuevo' );",
					"        }",
					"    })",
					"}"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"pm.test( 'Status code is OK' , () => {",
					"    pm.expect( pm.response.code ).to.be.below( 400 );",
					"} );",
					"",
					"pm.test( 'Response time is less than 200 ms' , () => {",
					"    pm.expect( pm.response.responseTime ).to.be.below( 200 );",
					"} );"
				]
			}
		}
